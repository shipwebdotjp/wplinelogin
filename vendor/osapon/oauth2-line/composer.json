{
    "name": "osapon/oauth2-line",
    "description": "LINE OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/osapon/oauth2-line",
    "authors": [
        {
            "name": "osapon",
            "email": "osa@osa-p.net",
            "homepage": "http://www.osa-p.net/"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "LINE"
    ],
    "minimum-stability": "beta",
    "require": {
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Osapon\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Osapon\\OAuth2\\Client\\Test\\": "tests/src/"
        }
    }
}
